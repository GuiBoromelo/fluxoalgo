                                                    <output expression="&quot;Categoria: Infantil&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <output expression="&quot;N&#227;o existe uma categoria referente &#224; sua idade&quot;" newline="True"/>
                                                </else>
                                            </if>
