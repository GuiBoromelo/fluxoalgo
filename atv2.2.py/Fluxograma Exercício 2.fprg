        <body>
            <declare name="idade" type="Integer" array="False" size=""/>
            <output expression="&quot;Entre com a sua idade para ver sua categoria:&quot;" newline="True"/>
            <input variable="idade"/>
            <if expression="idade &gt; 30">
                <then>
                    <output expression="&quot;Categoria: S&#234;nior&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="idade &gt;= 16">
